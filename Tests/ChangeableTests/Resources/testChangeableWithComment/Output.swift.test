struct A {
let a: String // in cm
let b: Int?
let c: Int? // this is optiona
var hasInt: Bool {
b != nil
}

    public func withChanges(
      a: String? = nil,
      b: (() -> Int?)? = nil,
      c: (() -> Int?)? = nil
    ) -> Self {
      Self (
        a: a ?? self.a,
        b: b != nil ? b!() : self.b,
        c: c != nil ? c!() : self.c
      )
    }
}
